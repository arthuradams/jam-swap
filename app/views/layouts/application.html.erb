<!DOCTYPE html>
<html data-ng-app="jamswap">
  <head>
    <title>JamSwap</title>
    <%= csrf_meta_tags %>
    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <script src="https://ajax.googleapis.com/ajax/libs/angularjs/1.3.15/angular.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/angular-ui-router/0.2.15/angular-ui-router.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/angular.js/1.5.0-beta.2/angular-resource.min.js"></script>
    <link href='https://fonts.googleapis.com/css?family=Luckiest+Guy' rel='stylesheet' type='text/css'>
    <base href="/">
    <%= javascript_include_tag "application" %>


  </head>
  <body>
    <div class="container">
     <div class = "full">
      <ul class="nav nav-tabs">
        <h1>JamSwap</h1>
        <li><h3><a data-ui-sref="categoryIndex" class="btn btn-primary">Categories</a></h2></li>
        <li><h3><a data-ui-sref="postIndex" class="btn btn-danger">Posts</a></h2></li>
        <li><h3><a data-ui-sref="commentIndex" class="btn btn-warning">Comments</a></h2></li>
        <li><h3><a data-ui-sref="userIndex" class="btn btn-default">Users</a></h2></li>
      </ul>
    </div>
      <div data-ui-view></div>
    </div>
  </body>
</html>
