<!DOCTYPE html>
<html data-ng-app="jamswap">
  <head>
    <title>JamSwap</title>
    <%= csrf_meta_tags %>
    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <script src="https://ajax.googleapis.com/ajax/libs/angularjs/1.3.15/angular.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/angular-ui-router/0.2.15/angular-ui-router.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/angular.js/1.5.0-beta.2/angular-resource.min.js"></script>
    <base href="/">
    <%= javascript_include_tag "application" %>
  </head>
  <body>
    <div class="container">
      <ul class="nav nav-tabs">
        <h1>JamSwap</h1>
        <li><a data-ui-sref="categoryIndex">Categories</a></li>
        <li><a data-ui-sref="postIndex">Posts</a></li>
        <li><a data-ui-sref="commentIndex">Comments</a></li>
        <li><a data-ui-sref="userIndex">Users</a></li>
      </ul>
      <div data-ui-view></div>
    </div>
  </body>
</html>
