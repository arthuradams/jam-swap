<main data-ng-controller="comment_controller as vm">
<h1>Comments</h1>
<table>
  <thead>
    <tr>
      <th>Comment</th>
      <th colspan="3"></th>
    </tr>
  </thead>
  <tbody>
      <tr data-ng-repeat="commentvm in vm.com_data">
        <td>{{commentvm.comment}}</td>
        <td><a data-ui-sref="commentShow({id: commentvm.id})">Show</a></td>
        <td><a data-ng-click="CommentShowViewModel.update(CommentShowViewModel.comment)">Edit</a></td>
        <td><a data-ng-click="CommentShowViewModel.destroy(CommentShowViewModel.comment)">Delete</a></td>
      </tr>
  </tbody>
</table>
</main>
