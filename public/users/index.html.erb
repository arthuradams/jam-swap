<main data-ng-controller="user_controller as vm">
<h1>Users</h1>
<table>
  <thead>
    <tr>
      <th>Email</th>
      <th colspan="3"></th>
    </tr>
  </thead>
  <tbody>
      <tr data-ng-repeat="user in vm.user_data">
        <td>{{user.email}}</td>
      </tr>
  </tbody>
  <br/>
</table>
<br/>
<form data-ng-submit="UserShowViewModel.create(UserShowViewModel.newUser)">
  <input placeholder="New User" data-ng-model="UserShowViewModel.newUser.user" />
  <button type="submit">Create</button>
</form>
